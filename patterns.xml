<?xml version="1.0" encoding="UTF-8"?>
<patterns>

  <!-- dot patterns -->
  <pattern type="dot" name="ドット小">
    <size>3</size>
    <spacing>15</spacing>
  </pattern>
  <pattern type="dot" name="ドット大">
    <size>6</size>
    <spacing>15</spacing>
  </pattern>

  <!-- stripe patterns -->
  <pattern type="stripe" name="ストライプななめ(／)">
    <lineWidth>18</lineWidth>
    <spacing>30</spacing>
    <angle>135</angle>
  </pattern>
<pattern type="stripe" name="ストライプななめ(＼)">
<lineWidth>18</lineWidth>
    <spacing>30</spacing>
    <angle>45</angle>
  </pattern>

  <!-- vertical lines -->
  <pattern type="vertical" name="ストライプたて">
    <lineWidth>24</lineWidth>
    <spacing>30</spacing>
  </pattern>

  <!-- horizontal lines -->
  <pattern type="horizontal" name="ストライプよこ">
    <lineWidth>24</lineWidth>
    <spacing>30</spacing>
  </pattern>

  <!-- check -->
  <pattern type="check" name="チェック">
    <spacing>20</spacing>
  </pattern>
  <pattern type="diacheck" name="チェックななめ">
    <spacing>30</spacing>

  </pattern>

